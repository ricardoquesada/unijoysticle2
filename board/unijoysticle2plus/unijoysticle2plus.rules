      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_800:450_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:450_um" "Via[0-1]_800:450_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:450_um-kicad_default" "Via[0-1]_800:450_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-POWER" "Via[0-1]_800:400_um" POWER
  )
  (via 
    "Via[0-1]_800:450_um-POWER" "Via[0-1]_800:450_um" POWER
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    POWER "Via[0-1]_800:450_um-POWER"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    GND "/ESP_EN" VBUS /USBD+ "/USBD-" "/J1_POTX" "/J1_FIRE" "/J1_POTY"
    "/J1_RIGHT" "/J1_LEFT" "/J1_DOWN" "/J1_UP" "/J2_POTX" "/J2_FIRE" "/J2_POTY" "/J2_RIGHT"
    "/J2_LEFT" "/J2_DOWN" "/J2_UP" "/RTS_PROG" "/ESP_BOOT" "/DTR_PROG" "/GPIO_J1_LED" "/GPIO_J2_LED"
    "/GPIO_SWITCH_JOY" "/GPIO_J1_RIGHT" "/GPIO_J2_POTY" "/ESP_TXD" "/ESP_RXD" "/GPIO_J2_POTX" "/GPIO_J1_LEFT" "/GPIO_J1_DOWN"
    "/GPIO_J2_RIGHT" "/GPIO_J1_POTY" "/GPIO_J2_FIRE" "/GPIO_J1_FIRE" "/GPIO_J2_UP" "/GPIO_J1_UP" "/GPIO_J2_DOWN" "/GPIO_J1_POTX"
    "/GPIO_J2_LEFT" "Net-(D2-Pad2)" "Net-(D3-Pad2)" "Net-(Q1-Pad2)" "Net-(Q2-Pad2)" "Net-(R7-Pad2)" "Net-(R8-Pad1)" "Net-(D1-Pad2)"
    "Net-(C8-Pad1)" "Net-(C12-Pad1)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class POWER
    +3V3 VDD
    (clearance_class POWER)
    (via_rule POWER)
    (rule
      (width 500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)